{
  "format_version": "1.10.0",
  "animations": {
    "idle": "animation.void_dragon.idle",
    "fly": "animation.void_dragon.fly",
    "attack": "animation.void_dragon.attack",
    "spawn": "animation.void_dragon.spawn"
  },
  "states": [
    {
      "name": "spawning",
      "animations": ["spawn"],
      "transitions": [
        {
          "if": "query.anim_time > 1.9",
          "play": "default"
        }
      ]
    },
    {
      "name": "default",
      "animations": ["idle"],
      "transitions": [
        {
          "if": "query.is_moving",
          "play": "flying"
        },
        {
          "if": "query.is_attacking",
          "play": "attacking"
        }
      ]
    },
    {
      "name": "flying",
      "animations": ["fly"],
      "transitions": [
        {
          "if": "!query.is_moving && !query.is_attacking",
          "play": "default"
        },
        {
          "if": "query.is_attacking",
          "play": "attacking"
        }
      ]
    },
    {
      "name": "attacking",
      "animations": ["attack"],
      "transitions": [
        {
          "if": "!query.is_attacking && query.is_moving",
          "play": "flying"
        },
        {
          "if": "!query.is_attacking && !query.is_moving",
          "play": "default"
        }
      ]
    }
  ]
}
